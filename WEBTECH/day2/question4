<script>
 const col = [
        "red",
        "green",
        "blue",
        "yellow",
        "orange",
        "purple",
        "pink",
        "cyan"
    ];
 function func(){
    const rand = Math.floor(Math.random() * col.length)
    document.body.style.backgroundColor=col[rand];
}
</script>
